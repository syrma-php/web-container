{
    "name": "syrma/web-container",
    "type": "library",
    "description": "Conterner of wep applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Tamás Szijártó",
            "email": "szijarto.tamas.developer@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "psr/log": "~1.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "react/http": "0.4.*",
        "monolog/monolog" : "1.*",
        "phpunit/phpunit":  "4.5.*",
        "zendframework/zend-diactoros":  "1.*",
        "fabpot/php-cs-fixer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Syrma\\WebContainer\\": "src/Syrma/WebContainer/"
        }
    },
    "suggest": {
        "ext-swoole": " Asynchronous & concurrent & distributed networking framework for PHP. http://www.swoole.com",
        "react/http": "Library for building an evented http server.",
        "zendframework/zend-diactoros": "PSR HTTP Message implementations"
    },

    "minimum-stability": "stable"
}
